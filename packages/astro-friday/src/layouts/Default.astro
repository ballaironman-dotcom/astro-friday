---
import Footer from '../components/Footer.astro'
import Header from '../components/Header.astro'
import BasicLayout from './Basic.astro'

interface Props {
  seo?: import('astro-seo').Props
}

const { seo } = Astro.props

---

<BasicLayout {seo}>
  <Header />

  <main class="px-1.75rem py-2.5rem">
    <slot />
    <Footer />
  </main>
</BasicLayout>
