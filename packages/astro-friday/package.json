{
  "name": "astro-friday",
  "type": "module",
  "version": "1.0.0-beta.7",
  "description": "A content-focused Astro integration with tag and series support.",
  "author": "Byron <byronogis@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/byronogis",
  "homepage": "https://github.com/byronogis/astro-friday#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/byronogis/astro-friday.git"
  },
  "bugs": "https://github.com/byronogis/astro-friday/issues",
  "keywords": [
    "astro",
    "withastro",
    "ui"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./routes/*": "./src/routes/*",
    "./layouts/*": "./src/layouts/*",
    "./components/*": "./src/components/*",
    "./integrations/unocss": "./src/integrations/unocss/index.ts"
  },
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "release": "pnpm publish",
    "prepare": "astro sync",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "@iconify-json/lucide": "catalog:icons",
    "@vercel/og": "catalog:seo",
    "astro": "catalog:build",
    "astro-seo": "catalog:seo",
    "dayjs": "catalog:deps",
    "nprogress": "catalog:frontend",
    "pixi.js": "catalog:frontend",
    "simplex-noise": "catalog:frontend",
    "unocss": "catalog:deps"
  },
  "dependencies": {
    "astro-color-scheme": "git+https://github.com/byronogis/astro-color-scheme.git#feat-support-astro-view-transition",
    "defu": "catalog:deps",
    "type-fest": "catalog:deps"
  },
  "devDependencies": {
    "@types/nprogress": "catalog:types",
    "typescript": "catalog:deps"
  }
}
